function mc_symbol_clone() {
	var clone = this._cloneProps(new this.constructor(this.mode, this.startPosition, this.loop));
	clone.gotoAndStop(this.currentFrame);
	clone.paused = this.paused;
	clone.framerate = this.framerate;
	return clone;
}

function getMCSymbolPrototype(symbol, nominalBounds, frameBounds) {
	var prototype = cjs.extend(symbol, cjs.MovieClip);
	prototype.clone = mc_symbol_clone;
	prototype.nominalBounds = nominalBounds;
	prototype.frameBounds = frameBounds;
	return prototype;
	}


(lib.電視指定機種 = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// Layer 1
	this.shape = new cjs.Shape();
	this.shape.graphics.f("#3D3A3A").s().p("");
	this.shape.setTransform(36.9,0.7);

	this.shape_1 = new cjs.Shape();
	this.shape_1.graphics.f("#3D3A3A").s().p("");
	this.shape_1.setTransform(21.9,0.6);

	this.shape_2 = new cjs.Shape();
	this.shape_2.graphics.f("#3D3A3A").s().p("Ag5A/IgDgDIgEgDQAMgJAFgOQAHgPABgSIAOABIgCAJIgCAJQAEAKAGAGQAGAGAHADIAAg1IghAAIAAgMIBPAAIAAAMIghAAIAAAUIAoAAIAAAMIgoAAIAAAZIAKABIAKAAIAIAAIALAAIAKAAIAJAAIgDAGIgCAHIghAAQgOAAgMgCQgLgCgIgGQgJgGgGgLQgEAJgFAIQgFAHgGAGIgEgDgAAugQIAAgUIhbAAIAAAUIgNAAIAAggIA1AAIgCgHIgDgGIAMgEIAEAJIAEAIIAxAAIAAAgg");
	this.shape_2.setTransform(7,0.5);

	this.shape_3 = new cjs.Shape();
	this.shape_3.graphics.f("#3D3A3A").s().p("");
	this.shape_3.setTransform(-8.1,0.6);

	this.shape_4 = new cjs.Shape();
	this.shape_4.graphics.f("#3D3A3A").s().p("");
	this.shape_4.setTransform(-23,0.6);

	this.shape_5 = new cjs.Shape();
	this.shape_5.graphics.f("#3D3A3A").s().p("");
	this.shape_5.setTransform(-37.8,0.8);

	this.timeline.addTween(cjs.Tween.get({}).to({state:[{t:this.shape_5},{t:this.shape_4},{t:this.shape_3},{t:this.shape_2},{t:this.shape_1},{t:this.shape}]}).wait(1));

